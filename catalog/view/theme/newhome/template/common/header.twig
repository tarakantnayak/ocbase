<!DOCTYPE html>
<html lang="en">
  <head>
    <meta charset="utf-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1">    
    <title>Home Entrepreneurs</title>
    
    <!-- Font awesome -->
    <link href="catalog/view/theme/newhome/newstyle/css/font-awesome.css" rel="stylesheet">
    <!-- Bootstrap -->
    <link href="catalog/view/theme/newhome/newstyle/css/bootstrap.css" rel="stylesheet">   
    <!-- SmartMenus jQuery Bootstrap Addon CSS -->
    <link href="catalog/view/theme/newhome/newstyle/css/jquery.smartmenus.bootstrap.css" rel="stylesheet">
    <!-- Product view slider -->
    <link rel="stylesheet" type="text/css" href="catalog/view/theme/newhome/newstyle/css/jquery.simpleLens.css">    
    <!-- slick slider -->
    <link rel="stylesheet" type="text/css" href="catalog/view/theme/newhome/newstyle/css/slick.css">
    <!-- price picker slider -->
    <link rel="stylesheet" type="text/css" href="catalog/view/theme/newhome/newstyle/css/nouislider.css">
    <!-- Theme color -->
	
	<link id="switcher" href="catalog/view/theme/newhome/newstyle/css/theme-color/lite-blue-theme.css" rel="stylesheet">
    <!-- <link id="switcher" href="catalog/view/theme/newhome/newstyle/css/theme-color/default-theme.css" rel="stylesheet">-->
    <!-- <link id="switcher" href="catalog/view/theme/newhome/newstyle/css/theme-color/bridge-theme.css" rel="stylesheet"> -->
    <!-- Top Slider CSS -->
    <link href="catalog/view/theme/newhome/newstyle/css/sequence-theme.modern-slide-in.css" rel="stylesheet" media="all">

    <!-- Main style sheet -->
    <link href="catalog/view/theme/newhome/newstyle/css/style.css" rel="stylesheet">    

    <!-- Google Font -->
    <link href='https://fonts.googleapis.com/css?family=Lato' rel='stylesheet' type='text/css'>
    <link href='https://fonts.googleapis.com/css?family=Raleway' rel='stylesheet' type='text/css'>
    
     <script src="https://ajax.googleapis.com/ajax/libs/jquery/2.1.1/jquery.min.js"></script>
<script src="https://maxcdn.bootstrapcdn.com/bootstrap/3.2.0/js/bootstrap.min.js"></script>
  

    <!-- HTML5 shim and Respond.js for IE8 support of HTML5 elements and media queries -->
    <!-- WARNING: Respond.js doesn't work if you view the page via file:// -->
    <!--[if lt IE 9]>
      <script src="https://oss.maxcdn.com/html5shiv/3.7.2/html5shiv.min.js"></script>
      <script src="https://oss.maxcdn.com/respond/1.4.2/respond.min.js"></script>
    <![endif]-->
  

  </head>
  <body> 
   <!-- wpf loader Two -->
    <div id="wpf-loader-two">          
      <div class="wpf-loader-two-inner">
        <span>Loading</span>
      </div>
    </div> 
    <!-- / wpf loader Two -->       
  <!-- SCROLL TOP BUTTON -->
    <a class="scrollToTop" href="#"><i class="fa fa-chevron-up"></i></a>
  <!-- END SCROLL TOP BUTTON -->


  <!-- Start header section -->
  <header id="aa-header">
    <!-- start header top  -->
    <div class="aa-header-top">
      <div class="container">
        <div class="row">
          <div class="col-md-12">
            <div class="aa-header-top-area">
              <!-- start header top left -->
              <div class="aa-header-top-left">
               
                <!-- start cellphone -->
                <div class="cellphone">
                <ul class="aa-head-top-nav-left">
								 <li><span class="fa fa-phone"></span>{{ telephone }}</li>								 
                                 </ul>
                </div>
                <!-- / cellphone -->
              </div>
              <!-- / header top left -->
              <div class="aa-header-top-right">
                <ul class="aa-head-top-nav-right">
                                 
                                 
                                 
                                    <li class="hidden-xs"><a href="{{ account }}">My Account</a></li>
                                    <li class="hidden-xs"><a href="{{ wishlist }}">Wishlist</a></li>
                                    <li class="hidden-xs"><a href="{{ shopping_cart }}">My Cart</a></li>
                                    <li class="hidden-xs"><a href="{{ checkout }}">Checkout</a></li>
									{% if logged %}
										<li class="hidden-xs"><a href="{{ logout }}" >Logout</a></li>
									 {% else %}
										<li class="hidden-xs"><a href="{{ login }}" >Login/Register</a></li>
									 {% endif %}
                                    <li class="hidden-sm hidden-md hidden-lg"><a href="{{ account }}"><i class="fa fa-user"></i></a></li>
                                    <li class="hidden-sm hidden-md hidden-lg"><a href="{{ wishlist }}"><i class="fa fa-heart"></i></a></li>
                                    
                                    <li class="hidden-sm hidden-md hidden-lg"><a href="{{ shopping_cart }}"><i class="fa fa-shopping-cart"></i></a></li>
									{% if logged %}
                                    <li class="hidden-sm hidden-md hidden-lg"><a href="{{ logout }}" ><i class="fa fa-sign-out"></i></a></li>
									{% else %}
									<li class="hidden-sm hidden-md hidden-lg"><a href="{{ login }}" ><i class="fa fa-sign-in"></i></a></li>
									{% endif %}
                </ul>
              </div>
            </div>
          </div>
        </div>
      </div>
    </div>
    <!-- / header top  -->

    <!-- start header bottom  -->
    <div class="aa-header-bottom">
      <div class="container">
        <div class="row">
          <div class="col-md-12">
            <div class="aa-header-bottom-area">
              <!-- logo  -->
              <div class="aa-logo">
                <!-- Text based logo -->
                <a href="{{ home }}">
                  <span class="fa fa-shopping-cart"></span>
                  <p>Home<strong>Entrepreneurs</strong> <span><b>Home Made Products, <i class="fa fa-heart"></i> Se</b></span></p>
                </a>
                <!-- img based logo -->
                <!-- <a href="index.html"><img src="catalog/view/theme/newhome/newstyle/img/logo.jpg" alt="logo img"></a> -->
              </div>
              <!-- / logo  -->
               <!-- cart box -->
              <div class="aa-cartbox hidden-xs">
                <a class="aa-cart-link" href="#">
                  <span class="fa fa-shopping-basket"></span>
                  <span class="aa-cart-title">SHOPPING CART</span>
                  <span class="aa-cart-notify">2</span>
                </a>
                <div class="aa-cartbox-summary">
                  {% if products  %}
				  <ul>
                    <li>
					{% for product in products %}
                      <a class="aa-cartbox-img" href="#"><img src="{{ product.thumb }}" alt="img"></a>
                      <div class="aa-cartbox-info">
                        <h4><a href="#">{{ product.name }}</a></h4>
                        <p>{{ product.quantity }} x {{ product.price }}</p>
                      </div>
                      <a class="aa-remove-product" onclick="cart.remove('{{ product.cart_id }}');"><span class="fa fa-times"></span></a>
					  {% endfor %}
                    </li>
                    <li>
					{% for total in totals %}
                      <span class="aa-cartbox-total-title">
                        {{ total.title }}
                      </span>
                      <span class="aa-cartbox-total-price">
                        {{ total.text }}
                      </span>
					  {% endfor %}
                    </li>
                  </ul>
                  <a class="aa-cartbox-checkout aa-primary-btn" href="{{ checkout }}">Checkout</a>
				                                      {%  else  %}
                                    <p class="text-center">{{ text_empty }}</p>
                                    {% endif %}

                </div>
              </div>
              <!-- / cart box -->
              <!-- search box -->

              <div class="aa-search-box" id="search">
                  <input type="text" name="search" value="{{ search }}" placeholder="{{ text_search }}">
                  <button type="submit"><span class="fa fa-search"></span></button>
              </div>
              <!-- / search box -->             
            </div>
          </div>
        </div>
      </div>
    </div>
    <!-- / header bottom  -->
  </header>
  <!-- / header section -->
  <!-- menu -->
  <section id="menu">
    <div class="container">
      <div class="menu-area">
        <!-- Navbar -->
        <div class="navbar navbar-default" role="navigation">
          <div class="navbar-header">
            <button type="button" class="navbar-toggle" data-toggle="collapse" data-target=".navbar-collapse">
              <span class="sr-only">Toggle navigation</span>
              <span class="icon-bar"></span>
              <span class="icon-bar"></span>
              <span class="icon-bar"></span>
            </button>          
          </div>
          <div class="navbar-collapse collapse">
            <!-- Left nav -->
            <ul class="nav navbar-nav">
				{% for category in categories %}
					{% if category.children %}
						<li><a href="{{ category.href }}">{{ category.name }} <span class="caret"></span></a>
							<ul class="dropdown-menu">
								{% for child in category.children %}
									<li><a href="{{ child.href }}">{{ child.name }}</a></li>
								{% endfor %}
							</ul>
						</li>
					{% else %}					
						<li><a href="{{ category.href }}">{{ category.name }}</a></li>
					{% endif %}
				{% endfor %}
				
				
							 
			  
            </ul>
          </div><!--/.nav-collapse -->
        </div>
      </div>       
    </div>
  </section>
  <!-- / menu -->

  </body>
</html>