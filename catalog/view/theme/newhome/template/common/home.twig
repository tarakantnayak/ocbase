{{ header }}
<!DOCTYPE html>
<html lang="en">
<!-- Start slider -->
<section id="aa-slider">
   <div class="aa-slider-area">
      <div id="sequence" class="seq">
         <div class="seq-screen">
            <ul class="seq-canvas">
               <!-- single slide item -->
               {% for image_data in slider %}
               <li>
                  <div class="seq-model">
                     <img data-seq src="{{ image_data.image }}" alt="Men slide img" />
                  </div>
                  <div class="seq-title">
                     <span data-seq>{{ image_data.content[0] }}</span>                
                     <h2 data-seq>{{ image_data.content[1] }}</h2>
                     <p data-seq>{{ image_data.content[2] }}</p>
                     <a data-seq href="{{ image_data.link }}" class="aa-shop-now-btn aa-secondary-btn">{{ image_data.content[3] }}</a>
                  </div>
               </li>
               {% endfor %}
            </ul>
         </div>
         <!-- slider navigation btn -->
         <fieldset class="seq-nav" aria-controls="sequence" aria-label="Slider buttons">
            <a type="button" class="seq-prev" aria-label="Previous"><span class="fa fa-angle-left"></span></a>
            <a type="button" class="seq-next" aria-label="Next"><span class="fa fa-angle-right"></span></a>
         </fieldset>
      </div>
   </div>
</section>
<!-- / slider -->
<!-- Start Promo section -->
