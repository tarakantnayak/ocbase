                        {% if category_num == 0 %}
                        <li class="active"><a href="#category_name_{{ category_num }}" data-toggle="tab">{{ category.name }}</a></li>
                        {% else %}
                        <li><a href="#category_name_{{ category_num }}" data-toggle="tab">{{ category.name }}</a></li>
                        {% endif %}
                        {% set category_num = category_num + 1 %}
